{
  "projectType": "application",
  "generators": {
    "@schematics/angular:application": {
      "strict": true
    }
  },
  "sourceRoot": "apps/AlledrogoFrontEnd/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/AlledrogoFrontEnd",
        "index": "apps/AlledrogoFrontEnd/src/index.html",
        "main": "apps/AlledrogoFrontEnd/src/main.ts",
        "polyfills": "apps/AlledrogoFrontEnd/src/polyfills.ts",
        "tsConfig": "apps/AlledrogoFrontEnd/tsconfig.app.json",
        "assets": [
          "apps/AlledrogoFrontEnd/src/favicon.ico",
          "apps/AlledrogoFrontEnd/src/assets"
        ],
        "styles": [
          "apps/AlledrogoFrontEnd/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/AlledrogoFrontEnd/src/environments/environment.ts",
              "with": "apps/AlledrogoFrontEnd/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "AlledrogoFrontEnd:build:production"
        },
        "development": {
          "browserTarget": "AlledrogoFrontEnd:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "AlledrogoFrontEnd:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/AlledrogoFrontEnd/src/test.ts",
        "polyfills": "apps/AlledrogoFrontEnd/src/polyfills.ts",
        "tsConfig": "apps/AlledrogoFrontEnd/tsconfig.spec.json",
        "karmaConfig": "apps/AlledrogoFrontEnd/karma.conf.js",
        "assets": [
          "apps/AlledrogoFrontEnd/src/favicon.ico",
          "apps/AlledrogoFrontEnd/src/assets"
        ],
        "styles": [
          "../node_modules/bootstrap/dist/css/bootstrap.min.css",
          "styles.css"
        ],
        "scripts": [
          "../node_modules/jquery/dist/jquery.min.js",
          "../node_modules/popper.js/dist/umd/popper.min.js",
          "../node_modules/bootstrap/dist/js/bootstrap.min.js"
        ]
      }
    }
  }
}
